{
  "name": "pocket-automation",
  "version": "1.0.0",
  "description": "Pocket automation",
  "main": "index.js",
  "scripts": {
    "stats": "node index.js --stats",
    "read": "node index.js --read",
    "package": "npm run _package-win && npm run _package-macos && npm run _package-linux",
    "_package-win": "pkg index.js --compress GZip -o dist/win/pocket-reader -t node16-win-x64",
    "_package-macos": "pkg index.js --compress GZip -o dist/macos/pocket-reader -t node16-macos-x64",
    "_package-linux": "copy node_modules\\open\\xdg-open dist\\xdg-open && pkg index.js --compress GZip -o dist/linux/pocket-reader -t node16-linux-x64"
  },
  "author": "Lukasz Nojek",
  "license": "ISC",
  "dependencies": {
    "arg": "^4.1.3",
    "chalk": "^4.1.0",
    "clipboardy": "^2.3.0",
    "express": "^4.17.1",
    "inquirer": "^7.3.3",
    "node-getpocket": "^1.0.0",
    "open": "^7.3.0",
    "ora": "^5.1.0",
    "stdio": "^0.2.7"
  }
}
